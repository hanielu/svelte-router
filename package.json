{
  "name": "@hvniel/svelte-router",
  "version": "0.0.2",
  "license": "MIT",
  "author": "Haniel Ubogu <https://github.com/HanielU>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HanielU/svelte-router.git"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run prepack",
    "build:dev": "vite build --mode development",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "prepack": "svelte-kit sync && svelte-package && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "test": "npm run test:unit -- --run"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@hvniel/vite-plugin-svelte-inline-component": "^0.0.16",
    "@iconify/json": "^2.2.351",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.22.5",
    "@sveltejs/package": "^2.3.12",
    "@sveltejs/vite-plugin-svelte": "^6.0.0",
    "@types/node": "^24.1.0",
    "@unocss/preset-mini": "^66.3.3",
    "@unocss/reset": "^66.3.3",
    "@vitest/browser": "^3.2.4",
    "bits-ui": "^2.8.11",
    "clsx": "^2.1.1",
    "mode-watcher": "^1.1.0",
    "playwright": "^1.54.1",
    "publint": "^0.3.2",
    "shiki": "^3.8.0",
    "svelte": "^5.36.17",
    "svelte-check": "^4.2.2",
    "tailwind-merge": "^3.3.1",
    "tailwind-variants": "^1.0.0",
    "typescript": "^5.0.0",
    "unocss": "^66.3.3",
    "unocss-preset-animations": "^1.2.1",
    "unplugin-icons": "^22.1.0",
    "vite": "^7.0.4",
    "vitest": "^3.2.4",
    "vitest-browser-svelte": "^1.0.0"
  },
  "keywords": [
    "svelte"
  ],
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
